  object qryPainel: TIBQuery
    Database = Dados.IBDatabase
    Transaction = Dados.IBTransaction
    BeforeOpen = qryPainelBeforeOpen
    OnCalcFields = qryPainelCalcFields
    SQL.Strings = (
      'select p.codigo,'
      '       p.datacadastro,'
      '       p.dataevento,'
      '       p.horaevendo,'
      '       a.codigoperfil,'
      '       pr.nome as nomeperfil,'
      '       tc.nome as nomeTipoConvidado,'
      '       e.nome as nomeEvento,'
      
        '       case when Exists(Select ag.codigo from tabagenda ag where' +
        ' ag.idpedidoagenda = p.codigo and ag.idperfil = a.codigoperfil a' +
        'nd ag.situacao <> 2) then 1 else 0 end as Agendado,'
      
        '       (Select First 1 ag.data from tabagenda ag where ag.idpedi' +
        'doagenda = p.codigo and ag.idperfil = a.codigoperfil and ag.situ' +
        'acao <> 2 and ag.data >= current_date order by ag.data) as Proxi' +
        'maDataAgenda,'
      '       a.datamelhordia,'
      '       a.horamelhordia,'
      '       p.Contato,'
      '       p.telefoneContato,'
      '       p.emailContato,'
      '       p.localevento,'
      '       Trim(case when p.horarioevento = 1 then '#39'MANH'#195#39
      '                 when p.horarioevento = 1 then '#39'ALMO'#199'O'#39
      '                 when p.horarioevento = 1 then '#39'TARDE'#39
      '                 when p.horarioevento = 1 then '#39'NOITE'#39
      '                 when p.horarioevento = 1 then '#39'OUTROS'#39
      '            else '#39#39' end) as HorarioEvento,'
      '       Case when a.id = 1 then'
      
        '          (select Count(a.id) from tabacompanhantespedidoagenda ' +
        'a where p.codigo = a.codigopedido and a.tipoagendamento = 2)'
      '       else 0 end as QtdeAcompanhantes'
      '  from tabpedidoagendamento p'
      
        '  left join tabacompanhantespedidoagenda a on p.codigo = a.codig' +
        'opedido'
      '  left join tabperfil pr on a.codigoperfil = pr.codigo'
      
        '  left join tabtipoconvidado tc on a.codigotipoconvidado = tc.co' +
        'digo'
      '  left join tabevento e on p.codigoevento = e.codigo'
      ' where a.tipoagendamento = 1'
      '--where'
      ''
      ' order by 1')
    Left = 279
    Top = 208
