  object qryHistorico: TIBQuery
    Database = Dados.IBDatabase
    Transaction = Dados.IBTransaction
    BeforeOpen = qryHistoricoBeforeOpen
    SQL.Strings = (
      
        'select H.*, U.USERNAME, COALESCE(S.DESCRICAO,H.descricaosituacao' +
        ') AS NOMESITUACAO, A.DATAFIM, A.HORAFIM, A.OBSERVACOES'
      '  from TABHISTORICOSITUACAO H'
      
        'INNER JOIN TABAVALIACAOPERFILGERAL A ON (H.CODIGOORIGEM = A.CODI' +
        'GOORIGEM)'
      'LEFT JOIN TABSITUACAOPRODUTO S ON (H.codigosituacao = S.CODIGO)'
      'LEFT JOIN TABUSUARIOS U ON (A.CODIGOUSUARIO = U.CODIGO)'
      ''
      'WHERE A.CODIGOREGISTRO = :CODIGOREGISTRO'
      ''
      'order by data, hora')
    Left = 292
    Top = 125
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'CODIGOREGISTRO'
        ParamType = ptUnknown
      end>
    object qryHistoricoCODIGOORIGEM: TIntegerField
