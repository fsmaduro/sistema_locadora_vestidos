  object qryAgenda: TIBQuery
    Database = Dados.IBDatabase
    Transaction = Dados.IBTransaction
    ForcedRefresh = True
    AfterScroll = qryAgendaAfterScroll
    BeforeOpen = qryAgendaBeforeOpen
    SQL.Strings = (
      
        'SELECT X.CODIGO,X.IDPEDIDOAGENDA, X.INICIO, X.FINAL, X.TIPO, X.N' +
        'OMEACAO, X.PERFIL,  X.OPCOES, X.COR, X.NomeSituacao, '
      '            X.DATAEVENTO, X.HORARIOEVENTO, X.DATA,'
      ''
      '       X.PERFIL ||'#39' - '#39'||'
      ''
      '       case'
      '         when e.nome is null then '#39#39
      '         else '#39'Evento: '#39'||e.nome||'#39' - '#39
      '       end ||'
      ''
      '       CASE'
      '         WHEN X.dataevento IS null THEN '#39#39
      
        '        ELSE '#39'Data Evento: '#39'|| EXTRACT(DAY FROM X.dataevento)||'#39 +
        '/'#39' || EXTRACT(MONTH FROM X.dataevento) ||'#39'/'#39' ||EXTRACT(YEAR FROM' +
        ' X.dataevento) ||  '#39' - '#39'||'
      ''
      '       CASE X.HORARIOEVENTO'
      '         WHEN 1 THEN '#39'Manh'#227#39
      '         WHEN 2 THEN '#39'Almo'#231'o'#39
      '         WHEN 3 THEN '#39'Tarde'#39
      '         WHEN 4 THEN '#39'Noite'#39
      '        END || '#39' - '#39
      '    END || X.CAPTION AS CAPTION'
      ''
      '  FROM('
      ''
      'SELECT DISTINCT A.CODIGO, A.IDPEDIDOAGENDA, A.DATA,'
      '   (A.DATA + A.HORA) AS INICIO,'
      '   (A.DATAFIM + A.HORAFIM) AS FINAL,'
      '   0 AS TIPO,'
      '   A.descricao AS NOMEACAO,'
      ''
      
        '   '#39'Perfil: '#39'||Coalesce(P.codigo,'#39#39')||'#39' - '#39'||Coalesce(P.NOME,'#39#39')' +
        '||'#39' - '#39'||'
      ''
      
        '   CASE WHEN AC.codigotipoconvidado IS null THEN '#39#39' ELSE '#39'Tipo C' +
        'onvidado: '#39'||CO.NOME|| '#39#39' END AS PERFIL,'
      ''
      
        '   Case when a.idpedidoagenda is null then '#39#39' else '#39' Pedido de A' +
        'genda: '#39'||A.idpedidoagenda end||'
      '   '#39' Situa'#231#227'o: '#39'||'
      
        '   Upper(Trim(Case when Coalesce(A.situacao,1) = 1 then '#39'Em Aber' +
        'to'#39
      
        '             when Coalesce(A.situacao,1) = 2 then '#39'Cancelada'#39'||'#39 +
        ' - Motivo: '#39'||Coalesce(a.motivocancelamento,'#39#39')'
      '             when Coalesce(A.situacao,1) = 3 then '#39'Confirmada'#39
      
        '             when Coalesce(A.situacao,1) = 4 then '#39'Finalizada'#39' e' +
        'lse '#39#39' End))||'#39' '#39'||'
      
        '   TRIM(CASE WHEN COALESCE((Select min(cs.sincronizado) from tab' +
        'contolesincagenda cs where cs.idagenda = a.codigo),0) = 1 THEN '#39 +
        '>>>> SINCRONIZADA <<<< <IDMASTER>'#39'||COALESCE(A.idmaster_sinc,'#39#39')' +
        '||'#39'</IDMASTER>'#39' ELSE '#39#39' END) AS CAPTION,'
      '   2 AS OPCOES,'
      
        '   Case when a.situacao = 1 and Coalesce(a.idpedidoagenda,0) > 0' +
        ' then :CORAGENDAPEDIDO'
      
        '        when a.situacao = 1 and Coalesce(a.idpedidoagenda,0) = 0' +
        ' then :CORAGENDATRABALHO'
      '        when a.situacao = 2 then :CORAGENDACANCELADA'
      '        when a.situacao = 3 then :CORCONFIRMADA'
      '        when a.situacao = 4 then :CORFINALIZADA'
      '        else 6610596 end as COR,'
      '   Trim(Case when A.situacao = 1 then '#39'Em Aberto'#39
      '             when A.situacao = 2 then '#39'Cancelada'#39
      '             when A.situacao = 3 then '#39'Confirmada'#39
      
        '             when A.situacao = 4 then '#39'Finalizada'#39' else '#39#39' End) ' +
        'as NomeSituacao,'
      ''
      
        ' COALESCE((SELECT DISTINCT E.DATAEVENTO FROM TABPEDIDOAGENDAMENT' +
        'OEVENTOS E WHERE E.CODIGO = A.IDPEDIDOAGENDA AND E.ID = A.IDEVEN' +
        'TO),PA.DATAEVENTO) AS DATAEVENTO,'
      ''
      
        ' COALESCE((SELECT DISTINCT E.HORARIOEVENTO FROM TABPEDIDOAGENDAM' +
        'ENTOEVENTOS E WHERE E.CODIGO = A.IDPEDIDOAGENDA AND E.ID = A.IDE' +
        'VENTO),PA.HORARIOEVENTO) AS HORARIOEVENTO,'
      ''
      
        '  COALESCE((SELECT DISTINCT E.codigoevento FROM TABPEDIDOAGENDAM' +
        'ENTOEVENTOS E WHERE E.CODIGO = A.IDPEDIDOAGENDA AND E.ID = A.IDE' +
        'VENTO),PA.codigoevento) AS codigoevento'
      ''
      ''
      'FROM TABAGENDA A'
      'LEFT JOIN tabperfil P on a.idperfil = p.codigo'
      'left join tabprodutos pr on a.codigoproduto = pr.codigo'
      
        'LEFT JOIN TABPEDIDOAGENDAMENTO PA ON (A.IDPEDIDOAGENDA = PA.CODI' +
        'GO)'
      
        'LEFT JOIN tabacompanhantespedidoagenda AC ON (PA.CODIGO =AC.CODI' +
        'GOPEDIDO AND AC.codigoperfil = P.CODIGO AND AC.tipoagendamento =' +
        ' 1)'
      
        'LEFT JOIN TABTIPOCONVIDADO CO ON (AC.CODIGOTIPOCONVIDADO = CO.CO' +
        'DIGO)'
      ''
      'where 1=1'
      '--where'
      ''
      ''
      ''
      'ORDER BY A.DATA'
      ''
      ''
      ''
      ') X'
      ''
      'left join tabevento e on (e.codigo = x.codigoevento)')
    Left = 722
    Top = 304
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'CORAGENDAPEDIDO'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'CORAGENDATRABALHO'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'CORAGENDACANCELADA'
