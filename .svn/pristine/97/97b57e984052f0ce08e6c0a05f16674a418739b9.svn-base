  object qryArara: TIBQuery
    Database = Dados.IBDatabase
    Transaction = Dados.IBTransaction
    BeforeOpen = qryAraraBeforeOpen
    SQL.Strings = (
      
        'Select a.*, p.codigo||'#39' - '#39'||p.nome as Perfil, ag.data, ag.hora,' +
        ' AG.DATAFIM, AG.HORAFIM'
      ' from TABARARA a'
      ' left join tabperfil p on a.codigoperfil = p.codigo'
      ' left join tabagenda ag on a.codigoagenda = ag.codigo'
      'where a.codigo = :codigo')
    Left = 348
    Top = 245
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'codigo'
        ParamType = ptUnknown
      end>
