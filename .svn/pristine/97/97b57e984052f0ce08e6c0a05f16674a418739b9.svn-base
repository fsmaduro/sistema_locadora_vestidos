  object qryAraraDetalhe: TIBQuery
    Database = Dados.IBDatabase
    Transaction = Dados.IBTransaction
    AfterOpen = qryAraraDetalheAfterOpen
    DataSource = dtsArara
    SQL.Strings = (
      'Select ad.codigoproduto,'
      '       p.descricao,'
      '       (Select First 1 pf.urlfoto'
      '          from tabprodutofotos pf'
      
        '          left join tabtipofoto tf on pf.codigotipofoto = tf.cod' +
        'igo'
      '         where pf.codigoproduto = ad.codigoproduto'
      '           and tf.usarnaarara = 1'
      '         order by pf.id) as UrlFoto'
      ' from tabararadetalhe ad'
      ' left join tabprodutos p on ad.codigoproduto = p.codigo'
      ''
      'where ad.codigoarara = :codigo'
      ''
      'order by ad.codigoproduto')
    Left = 356
    Top = 325
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'codigo'
        ParamType = ptUnknown
      end>
    object qryAraraDetalheCODIGOPRODUTO: TIntegerField
      FieldName = 'CODIGOPRODUTO'
      Origin = '"TABARARADETALHE"."CODIGOPRODUTO"'
      ProviderFlags = [pfInUpdate, pfInWhere, pfInKey]
      Required = True
    end
    object qryAraraDetalheDESCRICAO: TIBStringField
      FieldName = 'DESCRICAO'
      Origin = '"TABPRODUTOS"."DESCRICAO"'
      Size = 200
    end
    object qryAraraDetalheURLFOTO: TMemoField
      FieldName = 'URLFOTO'
      ProviderFlags = []
      BlobType = ftMemo
      Size = 8
    end
  end
